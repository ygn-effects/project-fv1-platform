{
  "name": "spinasm",
  "displayName": "spinasm",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
  "commands": [
    {
      "command": "spinasm.createProject",
      "title": "SpinASM: Create Project"
    },
    {
      "command": "spinasm.checkProjectSettings",
      "title": "SpinASM: Check Project Settings"
    },
    {
      "command": "spinasm.compileProgram0",
      "title": "SpinASM: Compile Program 0"
    },
    {
      "command": "spinasm.showSerialConfig",
      "title": "SpinASM: Show Serial Configuration"
    }
  ]
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "ini": "^5.0.0",
    "serialport": "^12.0.0"
  }
}
